   "id": "55c3d083",
   "metadata": {},
   "source": [
    "Answer -\n",
    "\n",
    "num_str = \"10\"\n",
    "num_int = int(num_str)\n",
    "print(num_int)  # Output: 10\n",
    "\n",
    "\n",
    "num_str = \"10.5\"\n",
    "num_float = float(num_str)\n",
    "print(num_float)  # Output: 10.5\n",
    "\n",
    "\n",
    "num_int = 10\n",
    "num_str = str(num_int)\n",
    "print(num_str)  # Output: \"10\""
